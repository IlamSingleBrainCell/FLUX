{
  "name": "FLUX Enterprise Platform",
  "short_name": "FLUX",
  "description": "Multi-Agent SDLC Platform for Enterprise Development Teams",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0f172a",
