      "description": "Manage epics and stories",
      "url": "/projects",
      "icons": [{ "src": "/icons/projects-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "AI Workspace",
      "short_name": "Workspace",
      "description": "Collaborate with AI agents",
      "url": "/workspace",
      "icons": [{ "src": "/icons/workspace-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "Performance insights",
      "url": "/analytics",
      "icons": [{ "src": "/icons/analytics-96x96.png", "sizes": "96x96" }]
    }
  ],
  "icons": [
    {
      "src": "/icons/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/manifest-icon-512.maskable.png",
