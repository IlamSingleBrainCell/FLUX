{
  "buildCommand": "ls -la && pwd && cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "echo 'Skipping root install'",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ]
}