{
  "version": 2,
  "name": "flux-multi-agent",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/chat",
      "dest": "/api/index.py"
    },
    {
      "src": "/agents",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "GROQ_API_KEY": "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",
    "NODE_ENV": "production"
  }
}