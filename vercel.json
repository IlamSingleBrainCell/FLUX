{
  "version": 2,
  "name": "flux-multi-agent",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/"
    },
    {
      "source": "/health",
      "destination": "/api/"
    },
    {
      "source": "/chat", 
      "destination": "/api/"
    },
    {
      "source": "/agents",
      "destination": "/api/"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}