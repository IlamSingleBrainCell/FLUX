{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq-api-key",
    "REDIS_URL": "@redis-url"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}